    <!-- описываем сам "запрос" -->
    <bean id="DEPART_EXECUTOR_FACT" class="pst.arm.client.modules.datagrid.domain.DTable" lazy-init="false">
        <property name="columnBuilders">
            <list>                
                <ref bean="BUILDER_DEPART_EXECUTOR_FACT__DEP_EXECUTOR_FACT_ID"/>
                <ref bean="BUILDER_DEPART_EXECUTOR_FACT__DEP_EX_PLAN_ID"/>
                <ref bean="BUILDER_DEPART_EXECUTOR_FACT__END_DATE"/>
                <ref bean="BUILDER_DEPART_EXECUTOR_FACT__FACT_VALUES"/>
                <ref bean="BUILDER_DEPART_EXECUTOR_FACT__COMMENT"/>
                <ref bean="BUILDER_DEPART_EXECUTOR_FACT__DATE_CORR"/>
                <ref bean="BUILDER_DEPART_EXECUTOR_FACT__USER_NAME"/>
                <ref bean="BUILDER_DEPART_EXECUTOR_FACT__USER_ID"/>  
                <ref bean="BUILDER_DEPART_EXECUTOR_FACT_WORK_PLAN__CONTRACT_STAGE_ID"/> 
            </list>
        </property>
        <property name="tables">
            <map>
                <entry key="MAIN" value="DEPART_EXECUTOR_FACT"/>
                <entry key="USERS" value="USERS"/>
                <entry key="WORK_PLAN" value="WORK_PLAN"/>
            </map>
        </property>
        <property name="tableJoins">
            <list>
                <bean class="pst.arm.client.modules.datagrid.domain.DTableJoin">
                    <property name="tableOne" value="MAIN"/>
                    <property name="tableTwo" value="USERS"/>
                    <property name="relationType" value="LEFT"/>
                    <property name="columnJoins">
                        <list>
                            <bean class="pst.arm.client.modules.datagrid.domain.DColumnJoin">
                                <property name="columnNameTableOne" value="USER_ID"/>
                                <property name="columnNameTableTwo" value="USER_ID"/>
                                <property name="columnRelationType" value="AND"/>
                            </bean>
                        </list>
                    </property>
                </bean>
                <bean class="pst.arm.client.modules.datagrid.domain.DTableJoin">
                    <property name="tableOne" value="MAIN"/>
                    <property name="tableTwo" value="WORK_PLAN"/>
                    <property name="relationType" value="LEFT"/>
                    <property name="columnJoins">
                        <list>
                            <bean class="pst.arm.client.modules.datagrid.domain.DColumnJoin">
                                <property name="columnNameTableOne" value="DEP_EX_PLAN_ID"/>
                                <property name="columnNameTableTwo" value="WORK_PLAN_ID"/>
                                <property name="columnRelationType" value="AND"/>
                            </bean>
                        </list>
                    </property>
                </bean>
            </list>
        </property>                   
        <property name="caption" value="Отметки о выполнении"/>            
        <property name="queryName" value="DEPART_EXECUTOR_FACT"/>
        <property name="countRowsInFiltrColumn" value="3"/>
        <property name="isFiltrShow" value="false"/>
        <property name="isShowReport" value="false"/>                   
    </bean>
    
