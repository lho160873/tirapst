	<subDataset name="Dataset" uuid="cc244b6a-5695-458e-94d6-78cdf0b642ce">
		<parameter name="ocpIdSub" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
     MAIN."OCP_ID" AS MAIN_OCP_ID,
     MAIN."DEPART_ID" AS MAIN_DEPART_ID,
     DEPART.SHORT_NAME AS DEPART_SHORT_NAME,
     MAIN."SUM_PLAN_LABOUR1" AS MAIN_SUM_PLAN_LABOUR1,
     MAIN."SUM_PLAN_COST1" AS MAIN_SUM_PLAN_COST1,
     MAIN."SUM_PLAN_COST_NORM1" AS MAIN_SUM_PLAN_COST_NORM1
FROM
     "OCP_DEPART_SUM" MAIN FULL JOIN DEPART AS DEPART ON MAIN.DEPART_ID=DEPART.DEPART_ID
WHERE
     MAIN.OCP_ID = $P{ocpIdSub}]]>
		</queryString>
		<field name="DEPART_SHORT_NAME" class="java.lang.String"/>
		<field name="MAIN_SUM_PLAN_LABOUR1" class="java.lang.String"/>
		<field name="MAIN_SUM_PLAN_COST1" class="java.lang.String"/>
	</subDataset>
	<parameter name="ocpId" class="java.lang.Integer"/>
	<parameter name="companyId" class="java.lang.String">
		<defaultValueExpression><![CDATA[$F{COMPANY_ID}]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    OCP.WORK_NAME,
    OCP.COMPANY_ID,
    ORDER_D.ORDER_NUMBER,
    ORDER_D.BASIS,
	OCP.ORDER_ID,
	ORGANIZATION.SHORT_NAME,
	FUNDING_SOURCE.NAME,
    ORDER_D.ORDER_ID,
    ORDER_D.WORKER_ID_GK,
    ORDER_D.WORKER_ID_RZ,
    W1.SHORT_NAME AS SN_1,
    W2.SHORT_NAME AS SN_2,
    DS1.SCS_STAGE_NUMBER AS DS1_EXPR,
    DS2.SCS_STAGE_NUMBER AS DS2_EXPR,
    DS3.SCS_STAGE_NUMBER AS DS3_EXPR,
    DS4.SCS_STAGE_NUMBER AS DS4_EXPR,
    DS5.SCS_STAGE_NUMBER AS DS5_EXPR,
    DS6.SCS_STAGE_NUMBER AS DS6_EXPR
FROM
     	OCP
	LEFT JOIN ORDER_D ON OCP.ORDER_ID = ORDER_D.ORDER_ID
	LEFT JOIN ORGANIZATION ON ORDER_D.CUSTOMER_ID = ORGANIZATION.ORGANIZATION_ID AND OCP.ORDER_ID=ORDER_D.ORDER_ID
	LEFT JOIN FUNDING_SOURCE ON OCP.FUNDING_SOURCE_ID=FUNDING_SOURCE.FUNDING_SOURCE_ID
    LEFT JOIN WORKER AS W1 ON ORDER_D.WORKER_ID_GK=W1.WORKER_ID AND ORDER_D.ORDER_ID = OCP.ORDER_ID
    LEFT JOIN WORKER AS W2 ON ORDER_D.WORKER_ID_RZ=W2.WORKER_ID AND ORDER_D.ORDER_ID = OCP.ORDER_ID
LEFT JOIN (SELECT
 SCS.STAGE_NUMBER AS SCS_STAGE_NUMBER,
 SCS.OCP_ID AS SCS_OCP_ID
  FROM SERV_CONTRACT_STAGE SCS LEFT JOIN OCP AS OCP ON SCS.OCP_ID=OCP.OCP_ID
  LEFT JOIN ORDER_D AS ORDER_D ON OCP.ORDER_ID=ORDER_D.ORDER_ID
  LEFT JOIN CONTRACT AS CONTRACT ON ORDER_D.CONTRACT_ID=CONTRACT.CONTRACT_ID) AS DS1
  ON DS1.SCS_OCP_ID=OCP.OCP_ID

  	LEFT JOIN (SELECT MAIN.CONTRACT_STAGE_ID AS MAIN_CONTRACT_STAGE_ID
, SCS.STAGE_NUMBER AS SCS_STAGE_NUMBER, SCS.OCP_ID AS SCS_OCP_ID
, MAIN.DEPART_ID AS MAIN_DEPART_ID
FROM WORK_PLAN MAIN
INNER JOIN SERV_CONTRACT_STAGE AS SCS ON MAIN.CONTRACT_STAGE_ID=SCS.CONTRACT_STAGE_ID
LEFT JOIN DEPART AS DEPART ON MAIN.DEPART_ID=DEPART.DEPART_ID) AS DS2
	ON DS2.SCS_OCP_ID=OCP.OCP_ID

    LEFT JOIN (SELECT SCS.OCP_ID AS SCS_OCP_ID
, MAIN.CONTRACT_STAGE_ID AS MAIN_CONTRACT_STAGE_ID, SCS.STAGE_NUMBER AS SCS_STAGE_NUMBER
, MAIN.ORGANIZATION_ID AS MAIN_ORGANIZATION_ID
FROM OCP_OUTER MAIN INNER JOIN SERV_CONTRACT_STAGE AS SCS ON MAIN.CONTRACT_STAGE_ID=SCS.CONTRACT_STAGE_ID
INNER JOIN ORGANIZATION AS ORGANIZATION_EXEC ON MAIN.ORGANIZATION_ID=ORGANIZATION_EXEC.ORGANIZATION_ID) AS DS3
	ON DS3.SCS_OCP_ID=OCP.OCP_ID

    LEFT JOIN (SELECT SCS.OCP_ID AS SCS_OCP_ID
 , MAIN.CONTRACT_STAGE_ID AS MAIN_CONTRACT_STAGE_ID, SCS.STAGE_NUMBER AS SCS_STAGE_NUMBER
  FROM OCP_PKI MAIN INNER JOIN SERV_CONTRACT_STAGE AS SCS ON MAIN.CONTRACT_STAGE_ID=SCS.CONTRACT_STAGE_ID
  INNER JOIN OCP_PKI_ST_SUM AS SUM ON MAIN.CONTRACT_STAGE_ID=SUM.CONTRACT_STAGE_ID) AS DS4
  ON DS4.SCS_OCP_ID=OCP.OCP_ID

    LEFT JOIN (SELECT MAIN.OCP_TRIP_ID AS MAIN_OCP_TRIP_ID, SCS.OCP_ID AS SCS_OCP_ID
, MAIN.CONTRACT_STAGE_ID AS MAIN_CONTRACT_STAGE_ID, SCS.STAGE_NUMBER AS SCS_STAGE_NUMBER
	FROM OCP_TRIP MAIN INNER JOIN SERV_CONTRACT_STAGE AS SCS ON MAIN.CONTRACT_STAGE_ID=SCS.CONTRACT_STAGE_ID
	INNER JOIN OCP_TRIP_SUMM AS SUMM ON MAIN.OCP_TRIP_ID=SUMM.OCP_TRIP_ID) AS DS5
	ON DS5.SCS_OCP_ID=OCP.OCP_ID

	LEFT JOIN (SELECT SCS.OCP_ID AS SCS_OCP_ID
, MAIN.CONTRACT_STAGE_ID AS MAIN_CONTRACT_STAGE_ID, SCS.STAGE_NUMBER AS SCS_STAGE_NUMBER
, MAIN.ORGANIZATION_ID AS MAIN_ORGANIZATION_ID
  FROM OCP_SERVICE MAIN INNER JOIN SERV_CONTRACT_STAGE AS SCS ON MAIN.CONTRACT_STAGE_ID=SCS.CONTRACT_STAGE_ID
  INNER JOIN ORGANIZATION AS ORGANIZATION_EXEC ON MAIN.ORGANIZATION_ID=ORGANIZATION_EXEC.ORGANIZATION_ID) AS DS6
  ON DS6.SCS_OCP_ID=OCP.OCP_ID
WHERE
     	OCP.OCP_ID = $P{ocpId}]]>
	</queryString>
	<field name="WORK_NAME" class="java.lang.String"/>
